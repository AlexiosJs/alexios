{
  "name": "alexios",
  "version": "1.0.0",
  "description": "A lightweight, powerful and zero-configuration React cli based on Webpack and babel.",
  "main": "lib/modules/index.js",
  "scripts": {
    "lib": "babel ./src -d ./lib --copy-files",
    "start": "watch \"npm run lib\" src --wait=2",
    "format:all": "prettier --write ./src ./bin",
    "lint:all": "eslint ./src --format=pretty",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "files": [
    "package.json",
    "package-lock.json",
    "LICENSE",
    "lib",
    "bin"
  ],
  "bin": "./bin/alexios.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexiosjs/alexios.git"
  },
  "author": "Cavendichun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexiosjs/alexios/issues"
  },
  "homepage": "https://github.com/alexiosjs/alexios#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node scripts/verify-commit.js"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/traverse": "^7.10.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "babel-preset-alexios": "^0.2.3",
    "body-parser": "^1.18.3",
    "chalk": "^4.0.0",
    "chokidar": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.5.3",
    "debug": "^3.1.0",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^9.0.0",
    "glob": "^7.1.2",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^4.2.0",
    "less-loader": "^6.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.4",
    "path-to-regexp": "^2.2.1",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "watch": "^1.0.2",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-progress-bar": "^1.2.1",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.0.4",
    "conventional-changelog": "^3.1.18",
    "conventional-changelog-cli": "^2.0.31",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "git-cz": "^4.3.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.5"
  }
}
